			compatible = "samsung,exynos4210-pd";
			reg = <0x10023ce0 0x20>;
			#power-domain-cells = <0x0>;
			label = "GPS";
		};

		gps-alive-power-domain@10023d00 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023d00 0x20>;
			#power-domain-cells = <0x0>;
			label = "GPS alive";
		};

		interrupt-controller@10490000 {
			compatible = "arm,cortex-a9-gic";
			#interrupt-cells = <0x3>;
			interrupt-controller;
			reg = <0x10490000 0x10000 0x10480000 0x10000>;
			cpu-offset = <0x4000>;
			phandle = <0x1>;
		};

		interrupt-controller@10440000 {
			compatible = "samsung,exynos4210-combiner";
			#interrupt-cells = <0x2>;
			interrupt-controller;
			reg = <0x10440000 0x1000>;
			samsung,combiner-nr = <0x14>;
			interrupts = <0x0 0x0 0x4 0x0 0x1 0x4 0x0 0x2 0x4 0x0 0x3 0x4 0x0 0x4 0x4 0x0 0x5 0x4 0x0 0x6 0x4 0x0 0x7 0x4 0x0 0x8 0x4 0x0 0x9 0x4 0x0 0xa 0x4 0x0 0xb 0x4 0x0 0xc 0x4 0x0 0xd 0x4 0x0 0xe 0x4 0x0 0xf 0x4 0x0 0x6b 0x4 0x0 0x6c 0x4 0x0 0x30 0x4 0x0 0x2a 0x4>;
			phandle = <0x9>;
		};

		pmu {
			compatible = "arm,cortex-a9-pmu";
			interrupt-parent = <0x9>;
			interrupts = <0x2 0x2 0x3 0x2 0x12 0x2 0x13 0x2>;
		};

		syscon@10010000 {
			compatible = "samsung,exynos4-sysreg", "syscon";
			reg = <0x10010000 0x400>;
			phandle = <0xc>;
		};

		system-controller@10020000 {
			compatible = "samsung,exynos4412-pmu", "syscon";
			reg = <0x10020000 0x4000>;
			interrupt-controller;
			#interrupt-cells = <0x3>;
			interrupt-parent = <0x1>;
			clock-names = "clkout0", "clkout1", "clkout2", "clkout3", "clkout4", "clkout8", "clkout9";
			clocks = <0x2 0x17 0x2 0x18 0x2 0x19 0x2 0x1a 0x2 0x1b 0x2 0x1 0x2 0x2>;
			#clock-cells = <0x1>;
			phandle = <0x7>;

			syscon-poweroff {
				compatible = "syscon-poweroff";
				regmap = <0x7>;
				offset = <0x330c>;
				mask = <0x5200>;
			};

			syscon-reboot {
				compatible = "syscon-reboot";
				regmap = <0x7>;
				offset = <0x400>;
				mask = <0x1>;
			};
		};

		dsi@11c80000 {
			compatible = "samsung,exynos4210-mipi-dsi";
			reg = <0x11c80000 0x10000>;
			interrupts = <0x0 0x4f 0x4>;
			power-domains = <0x8>;
			phys = <0xa 0x1>;
			phy-names = "dsim";
			clocks = <0x2 0x11e 0x2 0x8f>;
			clock-names = "bus_clk", "sclk_mipi";
			status = "disabled";
			#address-cells = <0x1>;
			#size-cells = <0x0>;
		};

