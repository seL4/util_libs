		tv-power-domain@10023c20 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c20 0x20>;
			#power-domain-cells = <0x0>;
			power-domains = <0x8>;
			label = "TV";
			phandle = <0x3d>;
		};

		cam-power-domain@10023c00 {
			compatible = "samsung,exynos4210-pd";
